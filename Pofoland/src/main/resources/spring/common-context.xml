<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<!-- PropertyManager 빈 설정 -->
	<bean id="propertyManager" class="com.hst.pofoland.common.utils.PropertyManager">
		<constructor-arg name="locations">
			<list>
				<value>db.server.properties</value>
				<value>network.path.properties</value>
			</list>
		</constructor-arg>
	</bean>

    <!-- PaginationManager 빈 설정 -->
    <bean id="paginationManager" class="egovframework.rte.ptl.mvc.tags.ui.pagination.DefaultPaginationManager">
        <property name="rendererType">
            <map>
                <entry key="text">
                    <bean class="com.hst.pofoland.common.renderer.CustomPaginationRenderer"/>
                </entry>
            </map>
        </property>
    </bean>


</beans>
